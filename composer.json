{
    "name": "qobo/cakephp-simple-slider",
    "description": "SimpleSlider plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "GPL-2.0",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "burzum/cakephp-file-storage": "^1.1",
        "burzum/cakephp-imagine-plugin": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "SimpleSlider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSlider\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
